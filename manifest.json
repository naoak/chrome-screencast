{
  "name": "Screencast (customized for local server)",
  "version": "0.3",
  "manifest_version": 2,
  "description": "Records a video of your chrome browser. Lets you share out the video or stills of the video.",
  "icons": {
    "128": "images/icon.png",
    "48": "images/icon.png",
    "16": "images/icon.png"
  },
  "background": {
    "scripts": ["screenshot.js"]
  },
  "browser_action": {
     "default_title": "Start recording.",
     "default_icon": "images/icon.png"
   },
  "permissions": ["tabs", "webNavigation", "<all_urls>", "storage"]
}
